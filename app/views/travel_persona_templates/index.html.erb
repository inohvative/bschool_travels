<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All travel personas
    </h1>

    <a href="/travel_personas/new" class="btn btn-block btn-outline-secondary">
      Add a new travel persona
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          User
        </th>

        <th>
          Party level
        </th>

        <th>
          Age cohort
        </th>

        <th>
          Activities
        </th>

        <th>
          City
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @travel_personas.each do |travel_persona| %>
      <tr>
        <!-- Display travel_persona.id start -->
        <td>
          <%= travel_persona.id %>
        </td>
        <!-- Display travel_persona.id end -->

        <!-- Display travel_persona.user_id start -->
        <td>
          <%= travel_persona.user.email %>
        </td>
        <!-- Display travel_persona.user_id end -->

        <!-- Display travel_persona.party_level_id start -->
        <td>
          <%= travel_persona.party_level_id %>
        </td>
        <!-- Display travel_persona.party_level_id end -->

        <!-- Display travel_persona.age_cohort_id start -->
        <td>
          <%= travel_persona.age_cohort_id %>
        </td>
        <!-- Display travel_persona.age_cohort_id end -->

        <!-- Display travel_persona.activities_id start -->
        <td>
          <%= travel_persona.activities_id %>
        </td>
        <!-- Display travel_persona.activities_id end -->

        <!-- Display travel_persona.city_id start -->
        <td>
          <%= travel_persona.city_id %>
        </td>
        <!-- Display travel_persona.city_id end -->

        <!-- Display travel_persona.created_at start -->
        <td>
          <%= time_ago_in_words(travel_persona.created_at) %> ago
        </td>
        <!-- Display travel_persona.created_at end -->

        <!-- Display travel_persona.updated_at start -->
        <td>
          <%= time_ago_in_words(travel_persona.updated_at) %> ago
        </td>
        <!-- Display travel_persona.updated_at end -->

        <td>
          <a href="/travel_personas/<%= travel_persona.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
