<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All energy levels
    </h1>

    <a href="/energy_levels/new" class="btn btn-block btn-outline-secondary">
      Add a new energy level
    </a>
  </div>
</div>

<!-- Ransack Code start -->
<div class="row">
  <div class="col-md-12 mb-3">
    <%= render partial: "energy_level_templates/energy_levels_search_form" %>
  </div>
</div>
<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Party level
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @energy_levels.each do |energy_level| %>
      <tr>
        <!-- Display energy_level.id start -->
        <td>
          <%= energy_level.id %>
        </td>
        <!-- Display energy_level.id end -->

        <!-- Display energy_level.party_level start -->
        <td>
          <%= energy_level.party_level %>
        </td>
        <!-- Display energy_level.party_level end -->

        <!-- Display energy_level.created_at start -->
        <td>
          <%= time_ago_in_words(energy_level.created_at) %> ago
        </td>
        <!-- Display energy_level.created_at end -->

        <!-- Display energy_level.updated_at start -->
        <td>
          <%= time_ago_in_words(energy_level.updated_at) %> ago
        </td>
        <!-- Display energy_level.updated_at end -->

        <td>
          <a href="/energy_levels/<%= energy_level.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @energy_levels, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
